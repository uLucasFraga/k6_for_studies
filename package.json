{
  "name": "k6-for-studies",
  "version": "0.0.1",
  "description": "Repository for examples by Performance Testing using K6",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules yarn.lock down",
    "docker:startServer": "docker run -d -p 3200:3000 --name serverest paulogoncalvesbh/serverest:latest",
    "docker:loadTest": "docker-compose up -d influxdb grafana && webpack && docker-compose run k6 run /dist/fullFlowLoad.test.js",
    "docker:stressTest": "docker-compose up -d influxdb grafana && webpack && docker-compose run k6 run /dist/fullFlowStress.test.js",
    "docker:up": "docker-compose up -d influxdb grafana",
    "docker:down": "docker-compose down -v",
    "startServer": "npx serverest@latest -p 3200 -y",
    "loadTest": "webpack && k6 run dist/fullFlowLoad.test.js",
    "stressTest": "webpack && k6 run dist/fullFlowStress.test.js",
    "loadTest:ci:wait": "wait-on -i 1000 -t 30000 -l http://localhost:3200"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uLucasFraga/k6_for_studies"
  },
  "author": "Lucas Fraga <ulucasfraga@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/uLucasFraga/k6_for_studies.git/README.md",
  "devDependencies": {
    "webpack-cli": "5.0.1",
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "babel-loader": "9.1.2"
  },
  "dependencies": {
    "casual": "1.6.2",
    "core-js": "3.27.1",
    "@faker-js/faker": "7.6.0",
    "webpack": "5.75.0"
  }
}